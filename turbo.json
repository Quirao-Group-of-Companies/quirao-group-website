{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["STRAPI_URL", "STRAPI_API_TOKEN"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**"]
    },
    "lint": {
      "cache": true
    },
    "check": {
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "types": {
      "cache": false
    }
  }
}
