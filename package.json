{
  "name": "quirao-group-monorepo",
  "private": true,
  "packageManager": "bun@1.3.8",
  "workspaces": [
    "apps/*",
    "packages/*",
    "cms/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=qgc-web",
    "dev:cms": "turbo run dev --filter=cms",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=qgc-web",
    "build:cms": "turbo run build --filter=cms",
    "format": "biome format --write .",
    "format:cms": "biome format --write ./apps/cms",
    "format:web": "biome format --write ./apps/web",
    "lint": "turbo run lint",
    "check:web": "biome check --write ./apps/web",
    "check:cms": "biome check --write ./apps/cms",
    "check": "biome check --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@tailwindcss/typography": "^0.5.19",
    "husky": "^9.1.7",
    "turbo": "latest"
  },
  "dependencies": {
    "@axiomhq/logging": "^0.2.0",
    "@axiomhq/nextjs": "^0.2.0",
    "@supabase/supabase-js": "^2.95.3",
    "resend": "^6.9.2",
    "tailwindcss": "^4.2.0",
    "web": "^0.0.2"
  }
}
